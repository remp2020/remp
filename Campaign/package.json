        "remp-campaign": "file:extensions/campaign-module"
    },
    "devDependencies": {
        "axios": "^0.21.1",
        "cross-env": "^7.0",
        "css-loader": "^6.0",
        "laravel-mix": "^6.0.12",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.8",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.59.2",
        "sass-loader": "^13.0",
        "vue-loader": "^15.9.8",
        "webpack": "^5.0.0",
        "webpack-cli": "^5.0.1"
    },
    "resolutions": {
        "@remp/js-commons": "portal:../Package/remp"
    }
