{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/*",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "extensions/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "remp/campaign-module": "@dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "josiasmontag/laravel-redis-mock": "~1.2",
        "larastan/larastan": "^3.3",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.9",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "ext-redis": "The extension required for better performance."
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "extensions/campaign-module/database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --tag=campaign-assets --force",
            "@php artisan service:post-install"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
