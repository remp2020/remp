{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "cache/predis-adapter": "^1.0",
        "deployer/deployer": "^6.0",
        "deployer/recipes": "^6.0",
        "doctrine/dbal": "~2.3",
        "fico7489/laravel-pivot": "^3.0",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "~1.4",
        "geoip2/geoip2": "~2.0",
        "guzzlehttp/guzzle": "^6.2",
        "kouz/laravel-airbrake": "^0.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.8.*",
        "laravelcollective/html": "5.8.*",
        "league/uri": "^5.0",
        "nicolaslopezj/searchable": "^1.13",
        "php-amqplib/php-amqplib": "^2.7",
        "piwik/device-detector": "^3.10",
        "predis/predis": " ~1.0",
        "recca0120/laravel-tracy": "^1.8",
        "remp/laravel-helpers": "*",
        "remp/laravel-sso": "*",
        "remp/laravel-widgets": "*",
        "remp/multi-armed-bandit": "*",
        "sentry/sentry-laravel": "^2.4",
        "spatie/laravel-searchable": "1.2.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "tightenco/ziggy": "^0.6.9",
        "ukfast/laravel-health-check": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "friendsofphp/php-cs-fixer": "2.14",
        "josiasmontag/laravel-redis-mock": "~1.2",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^7.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/tinker": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
