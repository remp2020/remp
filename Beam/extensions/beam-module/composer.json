{
    "name": "remp/beam-module",
    "description": "REMP Beam Laravel package",
    "type": "library",
    "homepage": "https://remp2030.com",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ans-group/laravel-health-check": "^2.0",
        "arrilot/laravel-widgets": "^3.13",
        "doctrine/dbal": "^3.1",
        "google/apiclient": "^2.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "laminas/laminas-diactoros": "^3.5",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^12.0",
        "league/uri": "^7.5",
        "matomo/searchengine-and-social-list": "^3.11",
        "predis/predis": "^2.3",
        "remp/laravel-helpers": "*",
        "remp/laravel-sso": "*",
        "remp/laravel-widgets": "*",
        "remp/remp-commons": "*",
        "sentry/sentry-laravel": "^4.11",
        "simshaun/recurr": "^5.0",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-searchable": "^1.9",
        "symfony/psr-http-message-bridge": "^v7.2",
        "symfony/yaml": "^7.2",
        "tightenco/ziggy": "^2.5",
        "yadakhov/insert-on-duplicate-key": "^1.2",
        "yajra/laravel-datatables-oracle": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^v1.20.0",
        "laravel/tinker": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^12.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Remp\\BeamModule\\": "src/",
            "Remp\\BeamModule\\Database\\Factories\\": "database/factories/",
            "Remp\\BeamModule\\Database\\Seeders\\": "database/seeders/",
            "Remp\\BeamModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Remp\\BeamModule\\BeamServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}