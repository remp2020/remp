{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-pcntl": "*",
        "arrilot/laravel-widgets": "^3.7",
        "barryvdh/laravel-cors": "^0.11.2",
        "deployer/deployer": "^6.0",
        "deployer/recipes": "^6.0",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "~1.4",
        "google/apiclient": "^2.0",
        "kouz/laravel-airbrake": "^0.4",
        "laracasts/flash": " ^3.0",
        "laravel/framework": "5.7.*",
        "laravelcollective/html": "5.7.*",
        "league/uri": "^5.0",
        "madewithlove/illuminate-psr-cache-bridge": "^1.0",
        "marc-mabe/php-enum": "^3.0",
        "matomo/searchengine-and-social-list": "^1.4",
        "php-amqplib/php-amqplib": "^2.7",
        "piwik/device-detector": "^3.10",
        "predis/predis": " ~1.0",
        "recca0120/laravel-tracy": "^1.8",
        "remp/journal": "*",
        "remp/laravel-helpers": "*",
        "remp/laravel-sso": "*",
        "remp/laravel-widgets": "*",
        "simshaun/recurr": "^3.0",
        "snowplow/referer-parser": "dev-master",
        "spatie/laravel-searchable": "1.2.3",
        "tightenco/ziggy": "^0.7.1",
        "yadakhov/insert-on-duplicate-key": "^1.2",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "friendsofphp/php-cs-fixer": "^2.14",
        "laravel/horizon": "^2.0",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
