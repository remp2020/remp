{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Subscribe user",
  "$id": "POST /api/v1/users/subscribe",
  "title": "Subscribe",
  "examples": [{
    "user_id": 1,
    "email": "test@test.com",
    "list_id": 1,
    "variant_id": 1,
    "send_welcome_email": true
  },
    {
      "user_id": 1,
      "email": "test@test.com",
      "list_code": "123",
      "variant_id": null
    }],
  "type": "object",
  "required": [
    "email",
    "user_id"
  ],
  "anyOf": [
    {
      "title": "Required list_id",
      "required": ["list_id"]
    },
    {
      "title": "Required list_code",
      "required": ["list_code"]
    }
  ],
  "additionalProperties": false,
  "properties": {
    "user_id": {
      "type": "integer"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "list_id": {
      "type": "integer"
    },
    "list_code": {
      "type": "string"
    },
    "variant_id": {
      "type": "integer"
    },
    "variant_code": {
      "type": "string"
    },
    "send_accompanying_emails": {
      "type": "boolean"
    }
  }
}
