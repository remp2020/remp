{
    "name": "nette/web-project",
    "description": "Nette: Standard Web Project",
    "type": "project",
    "license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.1",
        "nette/application": "^2.4",
        "nette/bootstrap": "^2.4.2",
        "nette/caching": "^2.5",
        "nette/database": "^2.4",
        "nette/di": "^2.4",
        "nette/finder": "^2.4",
        "nette/forms": "^2.4",
        "nette/http": "^2.4",
        "nette/mail": "^2.4",
        "nette/robot-loader": "^2.4 || ^3.0",
        "nette/security": "^2.4",
        "nette/utils": "^2.4",
        "latte/latte": "^2.4",
        "league/event": "~2.1",
        "tracy/tracy": "^2.4",
        "symfony/console": "^3.2",
        "predis/predis": "^1.1",
        "robmorgan/phinx": "^0.8.0",
        "vlucas/phpdotenv": "^2.4",
        "guzzlehttp/guzzle": "^6.2",
        "kdyby/autowired": "^1.4",
        "twig/twig": "^2.4",
        "mailgun/mailgun-php": "^3.0",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "remp/nette-sso": "*",
        "remp/multi-armed-bandit": "*",
        "remp/journal": "*",
        "deployer/deployer": "^6.0",
        "deployer/recipes": "^6.0",
        "tomaj/nette-api": "^1.11",
        "tomaj/hermes": "^2.1.1",
        "monolog/monolog": "^1.13",
        "php-amqplib/php-amqplib": "^2.7",
        "oops/webpack-nette-adapter": "^1.0",
        "embed/embed": "^3.3",
        "halaxa/json-machine": "^0.3",
        "justinrainbow/json-schema": "^5.2",
        "rootpd/nette-sentry": "^1.1"
    },
    "require-dev": {
        "nette/tester": "^1.6",
        "phpunit/phpunit": "^7.1",
        "symfony/var-dumper": "3.3.*",
        "phpstan/phpstan": "^0.11",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
