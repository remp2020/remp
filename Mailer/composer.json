{
    "name": "nette/web-project",
    "description": "Nette: Standard Web Project",
    "type": "project",
    "license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/nette-sso",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": ">= 5.6",
        "nette/application": "^2.4",
        "nette/bootstrap": "^2.4.2",
        "nette/caching": "^2.5",
        "nette/database": "^2.4",
        "nette/di": "^2.4",
        "nette/finder": "^2.4",
        "nette/forms": "^2.4",
        "nette/http": "^2.4",
        "nette/mail": "^2.4",
        "nette/robot-loader": "^2.4",
        "nette/security": "^2.4",
        "nette/utils": "^2.4",
        "latte/latte": "^2.4",
        "tracy/tracy": "^2.4",
        "symfony/console": "^3.2",
        "predis/predis": "^1.1",
        "robmorgan/phinx": "^0.8.0",
        "vlucas/phpdotenv": "^2.4",
        "guzzlehttp/guzzle": "^6.2",
        "bower-asset/jquery": "^3.1",
        "bower-asset/bootstrap": "^3.3",
        "bower-asset/jquery-bootstrap-purr": "^1.1",
        "bower-asset/waves": "^0.7.5",
        "bower-asset/malihu-custom-scrollbar-plugin": "^3.1",
        "bower-asset/animate.css": "^3.5",
        "bower-asset/material-design-iconic-font": "^2.2",
        "bower-asset/google-material-color": "^1.3",
        "bower-asset/jquery-placeholder": "^2.3",
        "bower-asset/autosize": "^3.0",
        "bower-asset/datatables.net": "^2.1",
        "bower-asset/bootstrap-select": "^1.12",
        "bower-asset/jquery.easy-pie-chart": "^2.1",
        "kdyby/autowired": "^1.4",
        "twig/twig": "^1.33",
        "mailgun/mailgun-php": "^2.3",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "remp/nette-sso": "*",
        "tomaj/nette-errbit": "~3.0"

    },
    "require-dev": {
        "nette/tester": "^1.6",
        "deployer/deployer": "^5.0",
        "deployphp/recipes": "dev-master",
        "friendsofphp/php-cs-fixer": "*"
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "www/assets/vendor",
                "bower-asset-library": "www/assets/vendor"
            }
        }
    }
}
